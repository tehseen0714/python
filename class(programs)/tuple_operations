class tuple_programs:

 def to_create_tuple(self,tuple):
  tuple=(1,2,3,4,5)
  return tuple
 
 def unpack_tuple(self,tuple):
  tuple=(1,2,3)
  a,b,c=tuple
  return a
  return b
  return c
  
 def reverse_tuple(self,tuple):
  tuple=(1,2,3,4,5)
  rev=tuple[::-1]
  return rev
 
 def slice_tuple(self,tuple):
   tuple=(1,2,3,4,5)
   return tuple[2:4]
 
 def to_check_element(self,tuple):
   tuple=(1,2,3,4,5)
   element=5
   if element in tuple:
     return element 
   
def remove_element(self,tuple):
  tuple=(1,2,3,4,5)
  number=3
  new_tuple=()
  for i in tuple:
    if i!=number:
     new_tuple=new_tuple+(i,)
    return new_tuple
 
 
obj= tuple_programs()
tuple=obj.to_create_tuple(tuple)
print("tuple",tuple)
 
variable=obj.unpack_tuple(tuple)
print("variables",variable)

reverse=obj.reverse_tuple(tuple)
print("reverse tuple",reverse)

slice_tuple=obj.slice_tuple(tuple)
print("sliced tuple",slice_tuple)

element=obj.to_check_element(tuple)
print("element:",element)

new_list=obj.remove_element(tuple)
print("new list:",new_list)



